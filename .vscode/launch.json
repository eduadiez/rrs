    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rrs-cli'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rrs-cli",
                    "--package=rrs-cli"
                ],
                "filter": {
                    "name": "rrs-cli",
                    "kind": "bin"
                }
            },
            "args": [
                "--binary",
                "riscv-sw-rust/riscv-std.bin",
                "-l",
                "executor.log"
            ],
            "cwd": "${workspaceFolder}"
        }
}